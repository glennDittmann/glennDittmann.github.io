---
import Rect from "./Rect.astro";

const {
  width = 200,
  height = 200,
  stroke = "black",
  strokeWidth = 4,
  fill = "none",
  ...rest
} = Astro.props;
---

<svg width={width} height={height} viewBox="-10 -10 120 120" {...rest}>
  <polygon
    points="
      0,0
      0,50
      25,50
      25,100
      75,100
      75,50
      100,50
      100,0
    "
    stroke={stroke}
    stroke-width={strokeWidth}
    fill={fill}
  >
  </polygon>
</svg>
