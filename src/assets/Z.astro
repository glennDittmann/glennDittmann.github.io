---
import Rect from "./Rect.astro";

const {
  width = 200,
  height = 200,
  stroke = "black",
  strokeWidth = 4,
  fill = "none",
  ...rest
} = Astro.props;
---

<Rect width={width} height={height} stroke={stroke} fill={fill} {...rest}>
  <line
    x1="0"
    y1="40"
    x2="70"
    y2="40"
    stroke={stroke}
    stroke-width={strokeWidth}></line>
  <line
    x1="100"
    y1="60"
    x2="30"
    y2="60"
    stroke={stroke}
    stroke-width={strokeWidth}></line>
</Rect>
