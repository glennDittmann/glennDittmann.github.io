---
import Rect from "./Rect.astro";

const {
  width = 200,
  height = 200,
  stroke = "black",
  strokeWidth = 4,
  fill = "none",
  ...rest
} = Astro.props;
---

<svg width={width} height={height} viewBox="-10 -10 120 120" {...rest}>
  <rect
    x="25"
    y="0"
    width="50"
    height="100"
    fill={fill}
    stroke={stroke}
    stroke-width={strokeWidth}
  >
  </rect>
  <line
    x1="25"
    y1="35"
    x2="75"
    y2="35"
    stroke={stroke}
    stroke-width={strokeWidth}></line>
</svg>
