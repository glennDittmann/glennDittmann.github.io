---
import Rect from "./Rect.astro";

const {
  width = 200,
  height = 200,
  stroke = "black",
  strokeWidth = 4,
  fill = "none",
  ...rest
} = Astro.props;
---

<svg width={width} height={height} viewBox="-10 -10 120 120" {...rest}>
  <polygon
    points="
      50,0
      50,50
      0,50
      0,100
      100,100
      100,0
    "
    stroke={stroke}
    stroke-width={strokeWidth}
    fill={fill}></polygon>
</svg>
