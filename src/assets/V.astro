---
import Rect from "./Rect.astro";

const {
  width = 200,
  height = 200,
  stroke = "black",
  strokeWidth = 4,
  fill = "none",
  ...rest
} = Astro.props;
---

<svg width={width} height={height} viewBox="-10 -10 120 120" {...rest}>
  <polygon
    points="
      0,0
      0,60
      25,100
      75,100
      100,60
      100,0
    "
    stroke={stroke}
    stroke-width={strokeWidth}
    fill={fill}></polygon>
  <line
    x1="50"
    y1="0"
    x2="50"
    y2="70"
    stroke={stroke}
    stroke-width={strokeWidth}></line>
  <line
    x1="40"
    y1="70"
    x2="60"
    y2="70"
    stroke={stroke}
    stroke-width={strokeWidth}></line>
</svg>
